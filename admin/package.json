{
	"name": "@vulpo-dev/auth-admin-dashboard",
	"version": "22.7.2-alpha",
	"main": "lib/lib.js",
	"types": "lib/lib.d.ts",
	"scripts": {
		"start": "parcel serve src/index.html --public-url /dashboard --dist-dir ./dist --hmr-port 2345",
		"clean": "rimraf ./dist ./lib",
		"build:lib": "tsc src/lib.ts --outDir ./lib --declaration",
		"build:dashboard": "parcel build --public-url /dashboard",
		"build": "npm run clean && npm run build:dashboard && npm run build:lib",
		"fmt": "rome format ./src --write",
		"check": "rome check ./src && tsc --noEmit"
	},
	"dependencies": {
		"@emotion/styled": "^11.10.5",
		"@parcel/transformer-typescript-types": "^2.8.3",
		"@phosphor-icons/react": "^2.0.6",
		"@reduxjs/toolkit": "^1.9.1",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.10",
		"@types/uuid": "^9.0.1",
		"@vulpo-dev/auth-react": "0.0.12",
		"@vulpo-dev/auth-sdk": "0.0.12",
		"@vulpo-dev/auth-ui": "0.0.18",
		"ky": "^0.33.2",
		"ky-universal": "^0.11.0",
		"open-props": "^1.5.5",
		"parcel": "^2.8.3",
		"react": "^18.2.0",
		"react-content-loader": "^6.2.1",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-router-dom": "^6.7.0",
		"rome": "^12.0.0",
		"typescript": "^4.7.4",
		"uuid": "^9.0.0",
		"werkbank": "^0.4.2"
	},
	"devDependencies": {
		"rimraf": "^4.1.2"
	}
}
